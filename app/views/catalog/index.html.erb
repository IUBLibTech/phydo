<%
# Override of catalog#index from curation_concerns.
# Adds a different search sidebar
%>

<% provide :page_title, "#{application_name} Search Results" %>
<% provide :page_class, 'search' %>
<% provide :page_header do %>
  <%= render 'navbar' %>
<% end %>

<% provide :sidebar do %>
  <%= render 'shared/filter_search' %>
  <%= render 'search_sidebar' %>
<% end %>

<div id="content">
  <%= render 'search_results' %>
</div>
